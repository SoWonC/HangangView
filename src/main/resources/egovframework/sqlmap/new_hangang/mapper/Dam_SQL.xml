<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="egovframework.new_hangang.mapper.DamMapper">

    <!-- 예시: 전체 조회 -->
    <select id="selectAllDam" resultType="egovframework.new_hangang.dto.DamDto">
        SELECT
            *
        FROM dam
    </select>

    <select id="selectDamSeGy" resultType="egovframework.new_hangang.dto.DamDto">
        SELECT
            *
        FROM dam
        where addr LIKE '%서울%' OR addr LIKE '%경기%';
    </select>

    <select id="selectfindDam" resultType="egovframework.new_hangang.dto.DamDto">
        SELECT
            *
        FROM dam
        where dmobscd =  #{dmobscd}
    </select>

</mapper>
